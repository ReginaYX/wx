/* pages/favorite/favorite.wxss */
.ul{
  margin-top: 30rpx;
}
.li{
  display: flex;
  align-items: center;
}
.li .item{
  height: 280rpx;
  padding-top: 10rpx;
  overflow: hidden;
  border-bottom: 1rpx solid #f8f8f8;
}
.li .pic{
  padding: 26rpx;
  height: 240rpx;
  width: 240rpx;
  background: no-repeat center center/100% auto;
  float: left;
  background-origin: content-box;
}
.li .content{
  float: left;
  color: #333;
  font-size: 11pt;
  padding: 0 20rpx 16rpx 20rpx;
  height: 240rpx;
  width: 400rpx;
}
.li .name{
  height: 76rpx;
  margin-top: 16rpx;
}
.li .price{
  /* padding-top: 30rpx; */
  color: #e93b3d; 
}
.li .symbol{
  font-size: 7pt;
  display: inline-block;
}
.ex_price{
  padding-top: 20rpx;
  text-decoration: line-through;
}
.ex_price .symbol{
  text-decoration: none;
}
.del{
  background: url(data:image/png;base64,) no-repeat center center/100% auto;
  width: 50rpx;
  height: 50rpx;
}
